{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "pingtest-frontend",
  "version": "0.1.0",
  "identifier": "com.pingtestri.rel",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "https://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "devtools": true,
        "title": "pingtest-frontend",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["default", "main-capability"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}